    <link href="css/main.css" rel="stylesheet" media="all">
</head>

<body>
    <div class="page-wrapper bg-gra-01 p-t-50 p-b-50 font-poppins">
        <div class="wrapper wrapper--w680">
            <div class="card card-4">
                <div class="card-body">
                    <h2 class="title">Registration Form - Hello employee!</h2>
                    <form method="POST">
                        <div class="row row-space">
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Nume</label>
                                    <input class="input--style-4" type="text" name="nume">
                                </div>
                            </div>
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Prenume</label>
                                    <input class="input--style-4" type="text" name="prenume">
                                </div>
                            </div>
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Username</label>
                                    <input class="input--style-4" type="text" name="username">
                                </div>
                            </div>
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Parolă</label>
                                    <input class="input--style-4" type="text" name="password">
                                </div>
                            </div>

                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Data angajării</label>
                                    <input class="input--style-4" type="date" name="data_angajarii">
                                </div>
                            </div>
    
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Data nașterii</label>
                                    <input class="input--style-4" type="date"  name="data_nasterii">
                                </div>
                            </div>

                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">CNP</label>
                                    <input class="input--style-4" type="text" name="cnp">
                                </div>
                            </div>

                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Gen</label>
                                    <div class="p-t-10">
                                        <label class="radio-container m-r-45">Masculin
                                            <input type="radio" checked="checked" name="gen" value="M">
                                            <span class="checkmark"></span>
                                        </label>
                                        <label class="radio-container">Feminin
                                            <input type="radio" name="gen" value="F">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                </div>
                            </div>
    

                        </div>
                 
                        <div class="col-3">
                            <div class="input-group">
                                <label class="label">Adresă</label>
                                <input class="input--style-4" type="text" name="adresa">
                            </div>
                        </div>

                        <div class="input-group">
                            <label class="label">Departament</label>
                            <div class="rs-select2 js-select-simple select--no-search">
                                <select name="departament_id">
                                    <option disabled="disabled" selected="selected">Alege un departament</option>
                                    <option value="1">Curățenie</option>
                                    <option value="2">Amenajări</option>
                                    <option value="3">Deratizare</option>
                                    <option value="4">Servicii medicale</option>
                                    <option value="5">Ambulanță veterinară</option>
                                    <option value="6">Termopane</option>
                                    <option value="7">Instalații termice</option>
                                </select>
                                <div class="select-dropdown"></div>
                            </div>
                        </div>
                        <div class="p-t-15">
                            <button class="btn btn--radius-2 btn--blue" type="submit">Join as an employee</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

    <!-- Jquery JS-->
    <script src="vendor/jquery/jquery.min.js"></script>
    <!-- Vendor JS-->
    <script src="vendor/select2/select2.min.js"></script>
    <script src="vendor/datepicker/moment.min.js"></script>
    <script src="vendor/datepicker/daterangepicker.js"></script>

    <!-- Main JS-->
    <script src="js/global.js"></script>

